<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>SeoCAT</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bulma@0.9.0/css/bulma.min.css">
    <link rel="stylesheet" href="assets/css/app.css">
    <link rel="stylesheet" href="assets/css/all.min.css">
    <script src="assets/js/utils.js"></script>
    <script src="assets/js/scanners.js"></script>
    <script src="assets/js/app.js"></script>
  </head>

  <body>

    <nav class="navbar" role="navigation" aria-label="main navigation">
      <div class="navbar-brand">
        <a class="navbar-item" href="/">
          <img src="https://cdn.glitch.com/84817e1c-c58f-4ede-b6e8-07c2999bce0b%2Fthumbnails%2Flogo.png?1597983579825">
          <h1 class="title-nav"><span class="or">SEO</span>Cat</h1>
        </a>
      </div>
    </nav>

    <div class="page">
      <div class="container con-w inbl">
        <div class="web-info">
          <div class="card">
            <div class="card-content">
              <div class="media">
                <div class="media-left">
                  <figure class="image is-64x64" id="wb-icon">

                  </figure>
                </div>
                <div class="media-content">
                  <p id="wb-title" class="title is-4"></p>
                  <p id="wb-url" class="subtitle is-6"></p>
                </div>
              </div>
          
              <div class="content" id="wb-desc"></div>
            </div>
          </div>
        </div>
  
        <div class="web-info">
          <div class="card">
            <div class="card-content">
              <p class="title is-4">Metadata</p>
  
              <div class="content">
                <div class="meta">
                  <span>Author</span>
                  <span class="right" id="mt-author"></span>
                </div>
  
                <div class="meta">
                  <span>Charset</span>
                  <span class="right" id="mt-charset"></span>
                </div>
  
                <div class="meta">
                  <span>Robots</span>
                  <span class="right" id="mt-robots"></span>
                </div>
  
                <div class="meta">
                  <span>Viewport</span>
                  <span class="right" id="mt-viewport"></span>
                </div>
              </div>
            </div>
          </div>
        </div>
  
        <div class="web-info">
          <div class="card">
            <div class="card-content">
              <p class="title is-4">Keywords (<span id="kw-counter">0</span>)</p>
          
              <div class="content" id="kw-list"></div>
            </div>
          </div>
        </div>
    </div>
  
        <div class="container inbl level-right is-fluid seo-result" id="msg-list">
        </div>
    </div>
</body>
</html>